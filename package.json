		"@typescript-eslint/eslint-plugin": "^8.36.0",
		"@typescript-eslint/parser": "^8.36.0",
		"builtin-modules": "^5.0.0",
		"electron": "^37.2.1",
		"esbuild": "^0.25.6",
		"esbuild-svelte": "^0.9.3",
		"obsidian": "^1.8.7",
		"svelte": "^4.2.18",
		"svelte-preprocess": "^6.0.3",
